    subgraph Problem1 [Проблема 1: Ввод неверного параметра при выкупе]
        P1_Start[Введен неверный параметр] --> P1_Step1[Касса: внести изменения<br>на основании сертификата]
        P1_Step1 --> P1_Step2[В ДМиМ появляется<br>новый слиток с верными данными]
        P1_Step2 --> P1_Step3[ДМиМ: Добавить новому слитку<br>UID/UIN со старого ошибочного]
        P1_Step3 --> P1_CheckUIN{УИН есть?}
        P1_CheckUIN -- Нет --> P1_Step3a[Обратиться в АС КЦ<br>для анализа]
        P1_CheckUIN -- Да --> P1_Step4[Направить письмо в ФПП<br>с документами и сертификатом]
        P1_Step3a --> P1_Step4
        P1_Step4 --> P1_End[Конец]
    end

    subgraph Problem2 [Проблема 2: Слиток без UID/UIN после корректировки]
        P2_Start[Слиток без UID/UIN] --> P2_Step1[Определить связь с выкупом?]
        P2_Step1 -- Нет --> P2_Step2[Причина – Касса]
        P2_Step2 --> P2_Step3[АС КЦ находит данные<br>и направляет в ДМиМ]
        P2_Step3 --> P2_Step4[ДМиМ вносит изменения]
        P2_Step4 --> P2_End[Конец]
    end

    subgraph Problem3 [Проблема 3: Старый слиток, данные утеряны при миграции]
        P3_Start[Данные утеряны] --> P3_Step1[АС КЦ ищет данные]
        P3_Step1 --> P3_Check{Данные есть?}
        P3_Check -- Нет --> P3_Unknown[Процедура не определена]
        P3_Check -- Да --> P3_Step2[Данные в ДМиМ]
        P3_Step2 --> P3_Step3[ДМиМ вносит изменения]
        P3_Step3 --> P3_End[Конец]
    end

    subgraph Problem4 [Проблема 4: Некорректная корректировка<br>слитков с '_']
        P4_Start[Присвоены не те УИН/УИД] --> P4_Step1[Анализ: запись в ДМиМ есть?]
        P4_Step1 --> P4_Check{Записи найдены?}
        P4_Check -- Нет --> P4_Step2[АС КЦ ищет данные -> ДМиМ]
        P4_Check -- Да --> P4_Step3[ДМиМ: перенос УИД/УИН<br>между записями]
        P4_Step2 --> P4_Step3
        P4_Step3 --> P4_Step4[Письмо в ФПП об изменении УИН]
        P4_Step4 --> P4_End[Конец]
    end

    subgraph Problem5 [Проблема 5: Данные о выкупе не ушли в ДМиМ]
        P5_Start[Некорр. справочник массы] --> P5_Step1[Касса корректирует -><br>в ДМиМ новая запись]
        P5_Step1 --> P5_Step2[Решить как Проблему 1]
        P5_Step2 --> P5_End[Конец]
    end

    subgraph Problem6 [Проблема 6: Дубли слитков]
        P6_Start[Обнаружен дубль] --> P6_Step1[Направить письмо в ФПП]
        P6_Step1 --> P6_Step2[Согласовать вывоз<br>с Филатовой, Горшковым, Кожиной]
        P6_Step2 --> P6_Step3[Кожина отключает<br>'рубильник' ВСП]
        P6_Step3 --> P6_Step4[Вывезти слиток в КИЦ/ГКЦ]
        P6_Step4 --> P6_End[Конец]
    end

    subgraph Problem7 [Проблема 7: Слиток не выведен из другой КО]
        P7_Start[Слиток чужой КО] --> P7_Step1[Направить письмо в ФПП]
        P7_Step1 --> P7_End[Конец]
    end

    subgraph Problem8 [Проблема 8: Слитки со времен АС ФС]
        P8_Start[Слиток не зарегистрирован] --> P8_Step1[АС КЦ: анализ и поиск атрибутов]
        P8_Step1 --> P8_Check{Данные найдены?}
        P8_Check -- Да --> P8_Step2[Передать в ДМиМ]
        P8_Check -- Нет --> P8_Step3[Найти решение для<br>первичной регистрации через ФПП]
        P8_Step2 --> P8_End[Конец]
        P8_Step3 --> P8_End
    end

    subgraph Problem9 [Проблема 9: Пересорт]
        P9_Start[Клиенту выдали не то] --> P9_Step1[Через ЕСЗЦК (900)<br>пригласить клиента]
        P9_Step1 --> P9_Step2[Обменять на корректные<br>слиток/сертификат]
        P9_Step2 --> P9_Check{Нужны весы?}
        P9_Check -- Да --> P9_Step3[Заказать оборудование]
        P9_Check -- Нет --> P9_End[Конец]
        P9_Step3 --> P9_End
    end

    subgraph Problem10 [Проблема 10: Ошибка ЕПК РБ]
        P10_Start[Клиент не клиент банка] --> P10_Step1[Ошибка ЕПК РБ<br>Решение запланировано на Q4 2025]
        P10_Step1 --> P10_Step2[Ждать обновления ЕПК<br>или найти Workaround]
        P10_Step2 --> P10_End[Конец]
    end

    subgraph Problem11 [Проблема 11: Слиток не зарегистрирован в ГИИС ДМДК]
        P11_Start[Нет УИН] --> P11_Step1[ДМиМ: анализ происхождения]
        P11_Step1 --> P11_Check{Внесен на ОМС?}
        P11_Check -- Да --> P11_Step2[Оставить на остатках<br>до решения проблемы в ГИИС]
        P11_Check -- Нет --> P11_Step3[Иная процедура?]
        P11_Step2 --> P11_End[Конец]
    end
